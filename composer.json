{
    "name": "faustbrian/collection-export",
    "description": "Export Collections into several file formats",
    "keywords": ["laravel", "framework", "Laravel-CollectionExport", "Laravel Collection Export", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de"
    }],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.3.*",
        "faustbrian/payload": "^1.0",
        "dompdf/dompdf": "~0.6.1",
        "h4cc/wkhtmltoimage-amd64": "0.12.x",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "maatwebsite/excel": "~2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\CollectionExport\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\CollectionExport\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
